REM   Script: Fantastic Foods
REM   Fantastic Foods

CREATE TABLE best_restaurants 
( 
	restaurant_rank	NUMBER(2), 
	restaurant_name	VARCHAR2(20), 
	country_name	VARCHAR2(20), 
	CONSTRAINT pk_restaurant_rank PRIMARY KEY (restaurant_rank) 
);

CREATE TABLE popular_foods 
( 
	food_rank	NUMBER(2), 
	food_name	VARCHAR(20), 
	country_name	VARCHAR(20), 	 
	CONSTRAINT pk_food_rank PRIMARY KEY (food_rank) 
);

